<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">
    <!-- Fill color -->
    <solid android:color="#FFFFFF"/>

    <!-- Border -->
    <stroke
        android:width="1dp"
        android:color="#005c90"/> <!-- change to outline color -->

    <!-- Optional size -->
    <size
        android:width="10dp"
        android:height="10dp"/>
</shape>
